#, c-format
msgid "Usage: %s <dir>\n"
msgstr "Usu: %s <dir>\n"

#: ../backends/gconf-merge-tree.c:107
#, c-format
msgid ""
"Usage: %s <dir>\n"
"  Merges a markup backend filesystem hierarchy like:\n"
"    dir/%%gconf.xml\n"
"        subdir1/%%gconf.xml\n"
"        subdir2/%%gconf.xml\n"
"  to:\n"
"    dir/%%gconf-tree.xml\n"
msgstr ""
"Usu: %s <dir>\n"
"  Combina'l marcáu del backend d'una xerarquía de sistema d'ficheru como:\n"
"    dir/%%gconf.xml\n"
"        subdir1/%%gconf.xml\n"
"        subdir2/%%gconf.xml\n"
"  en:\n"
"    dir/%%gconf-tree.xml\n"

